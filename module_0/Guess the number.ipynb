    "import numpy as np\n",
    "\n",
    "number = np.random.randint(1,101)\n",
    "print(\"The guessed number is from 1 to 100\")\n",
    "\n",
    "for count in range(1, 101):\n",
    "    if count == number:        \n",
    "        break\n",
    "\n",
    "print(f'You guessed the number {number} with {count} attempts')    "
   ]
